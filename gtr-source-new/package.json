{
  "name": "gtrweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --host 0.0.0.0 --disable-host-check",
    "build": "ng build --output-hashing=all",
    "copy-sitemap": "mkdir -p ./postmap/sitemap && cp ./dist/gtrweb/browser/sitemap/* ./postmap/sitemap",
    "copy-sitemap-php": "cp ./dist/gtrweb/browser/sitemap.php ./postmap",
    "copy-sitemap-xml": "cp ./dist/gtrweb/browser/sitemap.xml ./postmap",
    "copy-sitemap-xsl": "cp ./dist/gtrweb/browser/sitemap.xsl ./postmap",
    "copy-files": "npm run copy-sitemap && npm run copy-sitemap-php && npm run copy-sitemap-xml && npm run copy-sitemap-xsl",
    "move-files": "mv ./postmap/* ./dist/gtrweb/browser/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sitemap": "node sitemap-generator.js",
    "dev:ssr": "ng run gtrweb:serve-ssr",
    "serve:ssr": "node dist/gtrweb/server/main.js",
    "build:ssr": "npm run copy-files && ng build && ng run gtrweb:server && npm run move-files",
    "prerender": "ng run gtrweb:prerender"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^3.0.0-beta.0",
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^12.2.10",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^12.2.10",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/platform-server": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@hercilio/ngx-youtube-player": "^14.2.6",
    "@netcreaties/ngx-smart-banner": "^2.0.0-1",
    "@ngneat/cashew": "3.0.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@nguniversal/express-engine": "^12.1.0",
    "@w11k/angular-sticky-things": "^1.4.0",
    "ang-music-player": "^0.0.4",
    "angular-lightweight-youtube-embed": "^1.0.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.1",
    "express": "^4.15.2",
    "https": "^1.0.0",
    "jquery": "^3.6.0",
    "libphonenumber-js": "^1.11.5",
    "localstorage-polyfill": "^1.0.1",
    "moment": "^2.29.1",
    "ng-ticker": "^2.1.0",
    "ng2-adsense": "^10.0.1",
    "ngx-cookie-service": "^17.1.0",
    "ngx-google-analytics": "^11.2.1",
    "ngx-newsticker-albe": "^2.2.7",
    "ngx-owl-carousel": "^2.0.7",
    "ngx-owl-carousel-o": "^6.0.0",
    "ngx-ui-loader": "^11.0.0",
    "onesignal-ngx": "^2.0.1",
    "owl.carousel": "^2.3.4",
    "rxjs": "~6.6.0",
    "sessionstorage": "^0.1.0",
    "sweetalert2": "^11.14.4",
    "tslib": "^2.2.0",
    "typescript": "4.6.2",
    "video.js": "^7.17.2",
    "videojs-youtube": "^2.6.1",
    "xmlbuilder": "^15.1.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/googlemaps": "^3.39.12",
    "@types/jasmine": "~3.8.0",
    "@types/jquery": "^3.5.31",
    "@types/node": "^12.11.1",
    "@types/video.js": "^7.3.50",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0"
  }
}
